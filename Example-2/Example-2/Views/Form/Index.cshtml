@model List<ExampleTwo.Models.UserModel>

    <h2>Данни за студенти</h2>

    <table class="table table-bordered">
        <tr>
            <th>Id</th>
            <th>Години</th>
            <th>Име</th>
            <th>Фамилия</th>
            <th>Дата на раждане</th>
            <th>Ф. н.</th>
        </tr>
        @if (Model != null && Model.Any())
        {
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.Id</td>
                    <td>@user.Age</td>
                    <td>@user.FirstName</td>
                    <td>@user.LastName</td>
                    <td>@user.BirthDate</td>
                    <td>@user.FacultyNumber</td>
                </tr>
            }
        }
        else
        {
            <tr>
                <td colspan="6" class="text-center">
                    В системата няма въведени данни за студенти
                </td>
            </tr>
        }
    </table>

    <a asp-controller="Form" asp-action="Add" class="btn btn-success">
        Добавяне
    </a>